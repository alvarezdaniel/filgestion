<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="Fil.Modelo.Entidades.UnidadDeGestion, Fil.Modelo" table="UnidadesDeGestion" lazy="true">
    <id name="id" access="field">
      <column name="UdgId" sql-type="char(32)" 
        not-null="true"/>
      <generator class="uuid.hex" />
    </id>
    <property name="Autonumerico" column="UdgAutonumerico" insert="false" update="false" />
    <property name="Nombre" column="UdgNombre" />
  </class>
</hibernate-mapping>